#top
        glVertex2i(0, 50)
        glVertex2i(25, 50)

        glVertex2i(25, 50)
        glVertex2i(25, 25)

        glVertex2i(0, 25)
        glVertex2i(25, 25)

        glVertex2i(0, 25)
        glVertex2i(0, 25)


#right
 glVertex2i(25, 25)
        glVertex2i(50, 25)

        glVertex2i(50, 25)
        glVertex2i(50, 0)

        glVertex2i(25, 0)
        glVertex2i(50, 0)

        glVertex2i(25, 0)
        glVertex2i(25, 25)



